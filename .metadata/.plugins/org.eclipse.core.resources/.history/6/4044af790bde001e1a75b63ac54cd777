package main;

public class SinhVien {
	private String nam, lop;

	public SinhVien(String nam, String lop) {
		this.nam = nam;
		this.lop = lop;
	}

	public String getNam() {
		return nam;
	}

	public void setNam(String nam) {
		this.nam = nam;
	}

	public String getLop() {
		return lop;
	}

	public void setLop(String lop) {
		this.lop = lop;
	}
	
	
}
